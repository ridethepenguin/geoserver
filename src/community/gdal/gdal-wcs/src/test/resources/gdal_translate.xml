<GdalConfiguration>
    <gdalTranslateLocation>gdal_translate</gdalTranslateLocation>
    <!-- <gdalData>...</gdalData> -->
    <formats>
        <Format>
            <gdalFormat>JP2ECW</gdalFormat>
            <formatName>GDAL-JPEG2000</formatName>
            <fileExtension>.jp2</fileExtension>
            <singleFile>true</singleFile>
            <mimeType>image/jp2</mimeType>
            <type>binary</type> <!-- not really required, itâ€™s the default -->
        </Format>
        <Format>
            <gdalFormat>PDF</gdalFormat>
            <formatName>GDAL-PDF</formatName>
            <fileExtension>.pdf</fileExtension>
            <singleFile>true</singleFile>
            <mimeType>application/pdf</mimeType>
        </Format>
        <Format>
            <gdalFormat>MrSID</gdalFormat>
            <formatName>GDAL-MrSID</formatName>
            <fileExtension>.sid</fileExtension>
            <singleFile>true</singleFile>
            <mimeType>image/x-mrsid</mimeType>
        </Format>
        <Format>
            <gdalFormat>AAIGrid</gdalFormat>
            <formatName>GDAL-ArcInfoGrid</formatName>
            <fileExtension>.asc</fileExtension>
            <singleFile>false</singleFile>
        </Format>
        <Format>
            <gdalFormat>XYZ</gdalFormat>
            <formatName>GDAL-XYZ</formatName>
            <fileExtension>.txt</fileExtension>
            <singleFile>true</singleFile>
            <mimeType>text/plain</mimeType>
            <type>text</type>
        </Format>
    </formats>
</GdalConfiguration>